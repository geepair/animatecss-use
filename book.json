{
  "title": "Animate.css使用介绍",
  "author": "geepair",
  "description": "记录Animate.css的一些样式简单使用预览",
  "language": "zh-hans",
  "links": {},
  "styles": {
    "website": "styles/website.css",
    "ebook": "styles/ebook.css",
    "pdf": "styles/pdf.css",
    "mobi": "styles/mobi.css",
    "epub": "styles/epub.css"
  },
  "plugins": [
    "-search",
    "-sharing",
    "-lunr",
    "back-to-top-button",
    "expandable-chapters-small",
    "code",
    "copy-code-button",
    "search-pro",
    "github",
    "splitter",
    "sharing-plus",
    "tbfed-pagefooter",
    "flexible-alerts",
    "pageview-count",
    "hide-element",
    "simple-page-toc"
  ],
  "pluginsConfig": {
    "code": {
      "copyButtons": false
    },
    "github": {
      "url": "https://github.com/geepair"
    },
    "sharing": {
      "douban": true,
      "google": true,
      "linkedin": true,
      "twitter": false,
      "weibo": false,
      "all": [
        "douban", "facebook", "linkedin", "twitter", "weibo"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy ijava.icu 2020",
      "modify_label": "更新于：",
      "modify_format": "YYYY-MM-DD"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    }
  }
}
